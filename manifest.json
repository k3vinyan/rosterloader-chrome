{

  "manifest_version": 2,
  "name": "rosterloader-chrome",
  "version": "1.0",

  "description": "parse data and send to server",

  "icons": {
    "16": "icon16.png",
    "128": "icon128.png"

  },

  "content_scripts": [
    {
     "matches": ["http://localhost:9000/drivers/roster*"],
      "js": ["jquery-3.2.1.min.js", "rosterloader.js"]
    }
  ],
  "background":
  {
    "scripts": ["jquery-3.2.1.min.js"]
  }
  }
